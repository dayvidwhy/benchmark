{
  "exclude": ["node_modules"],
  "compilerOptions": {
      "jsx": "react",
      "lib": ["DOM", "ESNext"],
      "target": "es2020",
      "module": "esnext",
      "noImplicitAny": true,
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "types": ["node", "vite/client"],
      "strictNullChecks": true,
      "baseUrl": "."
  }
}
